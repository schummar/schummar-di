{
  "name": "@schummar/di",
  "version": "0.0.0",
  "description": "A simple dependency injection library for TypeScript.",
  "keywords": [
    "dependency-injection",
    "di",
    "inversion-of-control",
    "ioc",
    "typescript"
  ],
  "license": "ISC",
  "author": "Marco Schumacher <marco@schumacher.dev>",
  "repository": "https://github.com/schummar/schummar-di",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "runp lint:*",
    "lint:oxlint": "oxlint --type-aware --type-check",
    "lint:oxfmt": "oxfmt --check",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "build": "tsdown --clean --format=esm,cjs --exports"
  },
  "devDependencies": {
    "@nx/js": "^22.4.3",
    "@schummar/runp": "^2.2.4",
    "@swc-node/register": "^1.11.1",
    "@swc/core": "^1.15.11",
    "@vitest/coverage-v8": "^4.0.18",
    "nx": "^22.4.3",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "oxlint-tsgolint": "^0.11.3",
    "prettier": "^3.8.1",
    "publint": "^0.3.17",
    "tsdown": "^0.20.1",
    "vitest": "^4.0.18"
  },
  "volta": {
    "node": "24.13.0",
    "npm": "11.8.0",
    "pnpm": "10.28.2"
  }
}
